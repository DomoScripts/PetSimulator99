-- Gui to Lua
-- Version: 3.2

-- Instances:

local Main = Instance.new("ScreenGui")
local GengarsTycoon = Instance.new("Frame")
local Background = Instance.new("ImageLabel")
local Activate = Instance.new("ImageButton")
local TextLabel = Instance.new("TextLabel")
local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
local gengarlogo = Instance.new("ImageLabel")

-- Properties:

Main.Name = "Main"
Main.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
Main.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

GengarsTycoon.Name = "GengarsTycoon"
GengarsTycoon.Parent = Main
GengarsTycoon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
GengarsTycoon.BackgroundTransparency = 1.000
GengarsTycoon.BorderColor3 = Color3.fromRGB(27, 42, 53)
GengarsTycoon.Position = UDim2.new(0.344, 0, 0.335, 0)
GengarsTycoon.Size = UDim2.new(0.311, 0, 0.329, 0)
GengarsTycoon.Active = true
GengarsTycoon.Draggable = true

Background.Name = "Background"
Background.Parent = GengarsTycoon
Background.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Background.BackgroundTransparency = 1.000
Background.BorderColor3 = Color3.fromRGB(27, 42, 53)
Background.Size = UDim2.new(1, 0, 1, 0)
Background.ZIndex = 3
Background.Image = "rbxassetid://4966060682"
Background.ScaleType = Enum.ScaleType.Fit

Activate.Name = "Activate"
Activate.Parent = GengarsTycoon
Activate.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Activate.BackgroundTransparency = 1.000
Activate.Position = UDim2.new(0.189, 0, 0.678, 0)
Activate.Size = UDim2.new(0.622, 0, 0.513, 0)
Activate.ZIndex = 4
Activate.Image = "http://www.roblox.com/asset/?id=128286347071704"
Activate.ScaleType = Enum.ScaleType.Fit

TextLabel.Parent = GengarsTycoon
TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
TextLabel.BackgroundTransparency = 1.000
TextLabel.Position = UDim2.new(0, 0, 0.242, 0)
TextLabel.Size = UDim2.new(1, 0, 0.508, 0)
TextLabel.ZIndex = 4
TextLabel.Font = Enum.Font.GothamBlack
TextLabel.Text = "Auto-Buy Tycoon Buttons!"
TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
