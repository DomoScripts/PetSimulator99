gengarlogo.Name = "gengar logo"
gengarlogo.Parent = GengarsTycoon
gengarlogo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
gengarlogo.BackgroundTransparency = 1.000
gengarlogo.Position = UDim2.new(0.460, 0, 0.193, 0)
gengarlogo.Size = UDim2.new(0.727, 0, 0.727, 0)
gengarlogo.ZIndex = 3
gengarlogo.Image = "http://www.roblox.com/asset/?id=91911099692975"
gengarlogo.ImageTransparency = 0.630
gengarlogo.ScaleType = Enum.ScaleType.Fit

-- Functionality:

Activate.MouseButton1Click:Connect(function()
        for cannon = 1, 8 do
            -- For each cannon, apply upgrades in sequence
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Egg" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Speed_1" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_1" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_Radius_1" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Accuracy_1" })
    
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Egg" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Speed_2" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_2" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_Radius_2" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Accuracy_2" })
    
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Egg" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Speed_3" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_3" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_Radius_3" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Accuracy_3" })
    
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Egg" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Speed_4" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_4" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_Radius_4" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Accuracy_4" })
    
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Egg" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Speed_5" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_5" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Damage_Radius_5" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Cannon_" .. cannon .. "_Accuracy_5" })
    
            -- Purchase additional items
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "RainbowMachine" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "GoldMachine" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Possible_Loot_Board" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Leader_Board" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Leader_Board_2" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Second_Floor" })
            game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Rebirth_Shrine" })
    
    
            -- Purchase Breakable_Zone_1 from 1 to 6
            for i = 1, 6 do
                game:GetService("ReplicatedStorage").Network:FindFirstChild("Tycoons: Purchase"):InvokeServer({ [1] = "Breakable_Zone_" .. i })
            end
    
            wait(math.random(1, 2)) -- Random delay to avoid detection
        end
end)
